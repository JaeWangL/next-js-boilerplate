{
  "name": "next-js-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true yarn build",
    "start": "node node_modules/next/dist/bin/next start -p 8080",
    "lint:eslint": "eslint . --ext .ts,.js,.tsx,.jsx",
    "lint:eslint:fix": "eslint . --ext .ts,.js,.tsx,.jsx --fix",
    "prettify": "prettier --write **/*.{ts,tsx,js,jsx,md,json,yaml,yml}",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@emotion/cache": "^11.5.0",
    "@emotion/css": "^11.5.0",
    "@emotion/react": "^11.5.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@mui/material": "^5.0.6",
    "@reduxjs/toolkit": "^1.6.2",
    "@tanem/react-nprogress": "^3.0.81",
    "axios": "^0.21.1",
    "next": "^12.0.2",
    "next-i18next": "^8.10.0",
    "next-redux-wrapper": "^7.0.5",
    "next-seo": "^4.28.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-fast-compare": "^3.2.0",
    "react-redux": "^7.2.6",
    "react-toastify": "^8.1.0",
    "redux": "^4.1.2",
    "redux-observable": "^2.0.0",
    "sass": "^1.43.4"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.3.0",
    "@next/bundle-analyzer": "^12.0.2",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "babel-plugin-import": "^1.13.3",
    "eslint": "^8.1.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-next": "^12.0.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "lint-staged": "^11.2.6",
    "nextjs-sitemap-generator": "^1.3.1",
    "prettier": "^2.4.1",
    "redux-devtools-extension": "^2.13.9",
    "stylelint": "^14.0.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^23.0.0",
    "stylelint-prettier": "^1.2.0",
    "stylelint-scss": "^4.0.0",
    "typescript": "^4.4.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "npm run prettify",
      "npm run lint:eslint:fix"
    ],
    "*.{md,json,yaml,yml}": [
      "npm run prettify"
    ]
  }
}
